<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>

<VBox spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.atividadee2.controller.EqualizadorController">
    <children>
        <Label text="Equalizador - Pitty">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>

        <Label text="Cheiro:" />
        <TextField fx:id="txtfieldCheiro" />

        <Label text="Solta:" />
        <TextField fx:id="txtfieldSolta" />

        <Label text="Instrução:" />
        <TextField fx:id="txtfieldInstrucao" />

        <Button fx:id="btnEnviar" mnemonicParsing="false" onAction="#salvarNoCSV" text="Salvar no CSV" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>